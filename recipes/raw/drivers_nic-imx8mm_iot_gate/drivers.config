<config>

	<parent-provides>
		<service name="IRQ"/>
		<service name="IO_MEM"/>
		<service name="ROM"/>
		<service name="PD"/>
		<service name="RM"/>
		<service name="CPU"/>
		<service name="LOG"/>
		<service name="Timer"/>
		<service name="Uplink"/>
	</parent-provides>

	<default caps="100"/>

	<service name="Nic">
		<default-policy> <child name="eth1"/> </default-policy> </service>

	<start name="platform_drv" caps="350" managing_system="yes">
		<binary name="imx8mm_platform_drv"/>
		<resource name="RAM" quantum="4M"/>
		<provides>
			<service name="Platform"/>
		</provides>
		<config>
			<policy label="fec_nic -> " info="yes">
				<device name="fec"/>
			</policy>

			<policy label="usb_host_drv2 -> " info="yes">
				<device name="usb_host2"/>
			</policy>

			<policy label="gpio_drv1 -> " info="yes">
				<device name="gpio1"/>
			</policy>
		</config>
		<route>
			<any-service> <parent/> </any-service>
		</route>
	</start>

	<start name="gpio_drv1" caps="100">
		<binary name="imx_gpio_drv"/>
		<resource name="RAM" quantum="1M"/>
		<provides> <service name="Gpio"/> </provides>
		<config>
			<gpio num="10" mode="O" value="1"/>
		</config>
		<route>
			<service name="Platform"> <child name="platform_drv"/> </service>
			<any-service> <parent/> </any-service>
		</route>
	</start>

	<start name="fec_nic" caps="150">
		<binary name="fec_nic_drv"/>
		<resource name="RAM" quantum="17M"/>
		<config mac="02:00:00:00:00:00"/>
		<route>
			<service name="ROM" label="nic_drv.dtb"> <parent label="fec_nic_drv-imx8mm_iot_gate.dtb"/> </service>
			<service name="Platform"> <child name="platform_drv"/> </service>
			<service name="Uplink"> <parent/> </service>
			<any-service> <parent/> </any-service>
		</route>
	</start>

</config>
